<% content_for(:title) { "Edit #{@faculty.name} " } %>


<header class="header">
  <h1 class="header__heading"><%= content_for(:title) %></h1>
  <div class="header__actions">
    <%= render(:partial => 'admin/application/edit_toolbox_buttons', :locals => {:object => Faculty.find(params[:id]),:name_of_file=>Faculty.find(params[:id]).name.to_s,:blacklist_attributes=>"Password Digest,Activation Digest,Reset Digest,Reset Sent At,Remember Digest",:entity_attributes=>"Department,Faculty,Course"})%>

  </div>
</header>
<%= form_for(@faculty, :url => { :action => "update",:method => "put" }, html: {class: "form" })do |f| %>
  <%= render "form", f: f,parent: "edit"%>
<% end %>

