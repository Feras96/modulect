<% provide(:title, 'Modules') %>



<header class="header">
  <h1 class="header__heading" id="page-title">  Modules
</h1>
  <div class="header__actions">
    <a class="button" href="<%= new_admin_uni_module_path %>"><i class='fa fa-plus' aria-hidden='true'></i>&nbsp;&nbsp;New uni module</a>
  </div>
</header>


<div class="table-responsive">
  <table class="collection-data table" aria-labelledby="page-title">
    <thead>


    <tr>
          <th class="cell-label
        cell-label--string
        cell-label--false
        " scope="col">
            
                Name</th>
          <th class="cell-label
        cell-label--string
        cell-label--false
        " scope="col">
            Code</th>
          <th class="cell-label
        cell-label--string
        cell-label--false
        " scope="col">
            Lecturers</th>
      <th colspan="2" scope="col"></th>
    </tr>
    </thead>



<%= will_paginate %>
  <% @uni_modules.each do |uni_module| %>
    <tbody>
        <tr class="table__row"
            role="link"
            tabindex="0"
         data-url="<%= edit_admin_uni_module_path(uni_module) %>"
        >
              <td class="cell-data cell-data--string">
                
<%= uni_module.name %>

              </td>
              <td class="cell-data cell-data--string">
                

<%= uni_module.code %>

              </td>
              <td class="cell-data cell-data--string">
                

test
              </td>

          <td><span style="margin-right:10px;">
          <a class="action-edit" href="/admin/uni_modules/2/edit"><i class="fa fa-pencil" aria-hidden="true"></i>&nbsp;&nbspEdit</a>
          </span>
            <a class="table__action--destroy" data-confirm="Are you sure?" rel="nofollow" data-method="delete" href="/admin/uni_modules/2"><i class="fa fa-trash" aria-hidden="true"></i>&nbsp;&nbspDelete</a></td>
        </tr>




<% end %>
</tbody>
</table>

<%= will_paginate @uni_modules %>
</div>





