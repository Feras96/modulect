<%# A template for creating a uni_module
Contains render which contains fields which corresponds to a particular uni_module's attributes
%>
<% provide(:title, "New Module") %>
<header class="header">
  <h1 class="header__heading"><i class="fa fa-pencil-square" aria-hidden="true"></i>&nbsp;&nbsp;New Module </h1>
  <%= link_to "<i class='fa fa-times' aria-hidden='true' style='padding: 1px'></i>&nbsp;&nbsp;Quit".html_safe, :back, class: "button tool-box" %>
</header>
<div id="moduleForm" class="col-md-12">
  <%= form_for @uni_module, :url => { :action => "create",:method => "put" }, html: {class: "form" } do |f|%>
    <%= render "form", f: f,parent: "new"%>
  <% end %>
</div>
