<% provide(:title, "Edit #{@uni_module.code}") %>
<header class="header index-header-area">
  <h1 id="page-title">Edit <%= @uni_module.name %> </h1>
  <div class="header__actions">
  <%= link_to "<i class='fa fa-eye' aria-hidden='true' style='padding: 1px'></i>&nbsp;&nbsp;View Module".html_safe, @uni_module, method: :get, class: "button tool-box" %>
    <%= render(:partial =>
                   'admin/application/edit_toolbox_buttons',
               :locals => {:object => UniModule.find(params[:id]),
                           :name_of_file=>UniModule.find(params[:id]).name.tr(' ','').underscore,
                           :blacklist_attributes=>"More Info Link",
                           :entity_attributes=>"Course",
                           :has_many_attributes=>[]})%>

  </div>
</header>
<div id="moduleForm" class="col-md-12">
  <%= form_for @uni_module, :url => { :action => "update",:method => "put" }, html: {class: "form" } do |f|%>
    <%= render "form", f: f, parent: "edit"%>
  <% end %>
</div>



