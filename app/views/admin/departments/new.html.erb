<% content_for(:title) { "New User"} %>


<header class="header">
  <h1 class="header__heading"><%= content_for(:title) %></h1>
  <div class="header__actions">
    <%= link_to 'Back', :back, class: "button" %>
  </div>
</header>

<%= form_for(@department, html: {class: "form"}) do |f| %>
    <%= render "form", f: f,parent:"new"%>
<% end %>