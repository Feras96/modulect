  <% content_for(:title) { "New User"} %>


<header class="header">
  <h1 class="header__heading"><i class="fa fa-users" aria-hidden="true"></i>&nbsp;&nbsp;<%= content_for(:title) %></h1>
  <div class="header__actions">
    <%= link_to "<i class='fa fa-times' aria-hidden='true'></i>&nbsp;&nbsp;Quit".html_safe, :back, class: "button" %>
  </div>
</header>

  <%= form_for(@user, :url => { :action => "create",:method => "put" }, html: {class: "form",:onsubmit => "removeVoidFields()"}) do |f| %>
      <%= render "form", f: f,parent:"new"%>
  <% end %>